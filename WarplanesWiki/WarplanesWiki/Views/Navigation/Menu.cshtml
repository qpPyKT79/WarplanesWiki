@using WarplainsDomain.Entities;
@using WarplainsDomain.Helpers
@model IQueryable<WarplaneCategoty>


@Html.ActionLink("Главная", "List", "Warplane")
@foreach (var link in Model)
{
    @Html.RouteLink(link.Description(), new
{
    controller = "Warplane",
    action = "List",
    category = (int)link,
    page = 1
},
new
{
    @class = link == (WarplaneCategoty)ViewBag.SelectedCategory ? "selected" : null
})
}
